{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Default capabilities for the Quack desktop app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/quack-backend",
          "sidecar": true,
          "args": true
        }
      ]
    },
    "shell:allow-open"
  ]
}
