{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"target": "ES2020",
		"lib": [
			"ES2020",
			"DOM",
			"DOM.Iterable"
		],
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"src/*"
			]
		}
	},
	"include": [
		"src"
	],
	"exclude": [
		"dist",
		"node_modules"
	]
}